{% schema %}
{
  "name": "Multiple Blocks",
  "blocks": [
    {
      "type": "block-1",
      "name": "Block 1",
      "settings": [
        {
          "type": "text",
          "id": "title",
          "label": "Title"
        }
      ]
    },
    {
      "type": "block-2",
      "name": "Block 2",
      "settings": [
        {
          "type": "text",
          "id": "title",
          "label": "Title"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "multi-section",
      "category": "custom",
      "blocks": [
        {
          "type": "block-1",
          "settings": {
            "title": "Default Title for Block 1"
          }
        },
        {
          "type": "block-2",
          "settings": {
            "title": "Default Title for Block 2"
          }
        }
      ]
    }
  ]
}
{% endschema %}

{% for block in section.blocks %}
  {% if block.type == 'block-1' %}
    <h2>{{ block.settings.title }}</h2>
  {% elsif block.type == 'block-2' %}
    <h3>{{ block.settings.title }}</h3>
  {% endif %}
{% endfor %}
