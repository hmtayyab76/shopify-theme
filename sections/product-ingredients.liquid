<style>
     #shopify-section-{{ section.id }} *{
          padding: 0;
          margin: 0;
          box-sizing: border-box;
      }
      #shopify-section-{{ section.id }}{
          --padding-vertical-d9: {{ section.settings.padding-top-d }}rem;
          --padding-horizontal-d9: {{ section.settings.padding-left-d }}rem;
          --main-color-d9: {{ section.settings.main-color }};
          --text-color-d9: {{ section.settings.text-color }};
          --subtext-color-d9: {{ section.settings.subtext-color }};
          --second-color-d9: {{ section.settings.second-color }};
          --heading-font-d9: {{ section.settings.heading-size-d }}px;
          --text-font-d9: {{ section.settings.text-size-d }}px;
          --btn-font-d9: {{ section.settings.btn-size-d }}px;
          --heading-weight-d9: {% if section.settings.heading-weight-d == 'bold' %}bold{% else %}light{% endif %};
          --btn-weight-d9:  {% if section.settings.heading-weight-d == 'bold' %}bold{% else %}light{% endif %};
  }
  #shopify-section-{{ section.id }} .pi-37-d9-main{
      width: 100%;
      background: {{ section.settings.bg-color }};
  }
  #shopify-section-{{ section.id }} .pi-37-d9-inner{
      max-width: 1140px;
      width: 95%;
      margin: 0 auto;
      padding: var(--padding-vertical-d9) var(--padding-horizontal-d9);
  }
  #shopify-section-{{ section.id }} .pi-37-d9-container {
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 60px;
  }

  #shopify-section-{{ section.id }} .pi-37-d9-ingredients {
      width: 50%;
      padding: 20px;
  }

  #shopify-section-{{ section.id }} .pi-37-d9-pills button {
      display: inline-block;
      padding: 7px 15px;
      font-size: calc(var(--text-font-d9) + 1px);
      background: var(--second-color-d9);
      border: 1px solid var(--main-color-d9);
      border-radius: 20px;
      cursor: pointer;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-pills {
      display: flex;
      align-items: center;
      justify-content: flex-start;
      gap: 10px;
      flex-wrap: wrap;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-pills button:first-child{
      margin-left: 0;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-pills button.active-pill-d9 {
      background: var(--main-color-d9);
      color: white;
  }
  #shopify-section-{{ section.id }} .pi-37-slider-container{
      width: 40%;
      position: relative;
      border: 1px solid var(--main-color-d9);
      border-radius: 30px;
      background: var(--second-color-d9);
      margin-bottom: 30px;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-slider {
      width: 100%;
      overflow: hidden;
  }

  #shopify-section-{{ section.id }} .pi-37-d9-slides {
      display: flex;
      transition: transform 0.5s ease-in-out;
  }

  #shopify-section-{{ section.id }} .pi-37-d9-slide {
      min-width: 100%;
      box-sizing: border-box;
      padding: 10px;
  }

  #shopify-section-{{ section.id }} .pi-37-d9-indicators {
      display:{% if section.settings.dots-d %}flex{% else %}none{% endif %};
      position: absolute;
      bottom: -50px;
      left: 50%;
      transform: translateX(-50%);
  }

  #shopify-section-{{ section.id }} .pi-37-d9-dot {
      height: 15px;
      width: 15px;
      margin: 0 5px;
      background-color: #bbb;
      border-radius: 50%;
      display: inline-block;
      cursor: pointer;
  }

  #shopify-section-{{ section.id }} .pi-37-d9-dot.active {
      background-color: var(--main-color-d9);
  }

  #shopify-section-{{ section.id }} .pi-37-d9-arrows {
      display: {% if section.settings.arrows-d %} flex{% else %}none {% endif %};
      position: absolute;
      width: 100%;
      gap: 40px;
      {% if section.settings.arrow-position == 'bottom' %}
      bottom: -26px;
      justify-content: center;
      {% else %}
      justify-content: space-between;
      bottom: 50%;
      transform: translatey(50%);
      {% endif %}
  }

  #shopify-section-{{ section.id }} .pi-37-d9-arrow {
      background: rgba(0, 0, 0, 0.9);
      border: none;
      color: white;
      padding: 10px;
      cursor: pointer;
      display: flex;
      align-items: center;
      justify-content: center;
      border-radius: 50%;
      width: 46px;
      height: 46px;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-arrow svg{
      fill: #fff;
  }

  #shopify-section-{{ section.id }} .pi-37-d9-arrow.not-available-d9 {
      background: rgba(0, 0, 0, 0.2);
      cursor: not-allowed;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-arrow svg{
      width: 14px;
      height: 14px;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-slides.grabbing {
      cursor: grabbing;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-slides.grabbing *{
      user-select: none !important;
  }
  #shopify-section-{{ section.id }} .img-content-pi-37-d9{
      width: 70%;
      margin: 30px auto;
      overflow: hidden;
      border-radius: 50%;
      border: 5px solid {{ section.settings.img-color }};
      aspect-ratio: 1/1;
  }
  #shopify-section-{{ section.id }} .img-content-pi-37-d9 img{
      width: 100%;
      height: 100%;
      object-fit: contain;
      aspect-ratio: 1/1;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-arrow-left{
      transform: rotate(180deg);
  }
  #shopify-section-{{ section.id }} .pi-37-d9-slide h2,#shopify-section-{{ section.id }}  .pi-37-d9-slide p{
      text-align: center;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-slide h2,#shopify-section-{{ section.id }}  .pi-37-d9-slide p,#shopify-section-{{ section.id }}  .text-div-d9 p,#shopify-section-{{ section.id }}  .pi-37-d9-ingredients h2{
      margin: 10px 0;
      line-height: 130%;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-slide h2,#shopify-section-{{ section.id }}  .pi-37-d9-ingredients h2{
      color: var(--text-color-d9);
      font-size: var(--heading-font-d9);
      font-weight: var(--heading-weight-d9);
      margin-bottom: 20px;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-slide p,#shopify-section-{{ section.id }}  .text-div-d9 p{
      color: var(--subtext-color-d9);
      font-size: var(--text-font-d9);
  }
  #shopify-section-{{ section.id }} .btn-div-d9{
      display: flex;
      align-items: center;
      justify-content: flex-start;
      gap: 20px;
      margin-top: 30px;
  }
  #shopify-section-{{ section.id }} .pi-37-d9-main-btn{
      text-decoration: none;
      border: 1px solid var(--main-color-d9);
      border-radius: 50px;
      transition: all .4s;
      padding: 12px 25px;
      display: block;
      min-width: 120px;
      text-align: center;
      font-size: var(--btn-font-d9);
      font-weight: var(--btn-weight-d9);
  }
  #shopify-section-{{ section.id }} .pi-37-d9-main-btn.style-btn-1-d9{
      background: var(--main-color-d9);
      color: var(--second-color-d9);
  }
  #shopify-section-{{ section.id }} .pi-37-d9-main-btn.style-btn-2-d9{
      color: var(--main-color-d9);
      background: var(--second-color-d9);
  }
  #shopify-section-{{ section.id }} .pi-37-d9-main-btn.style-btn-2-d9:hover{
      background: var(--main-color-d9);
      color: var(--second-color-d9);
  }
  #shopify-section-{{ section.id }} .pi-37-d9-main-btn.style-btn-1-d9:hover{
      color: var(--main-color-d9);
      background: var(--second-color-d9);
  }
  #shopify-section-{{ section.id }} .hide-on-d-d9{
      display: none;
  }
  #shopify-section-{{ section.id }} .hide-on-m-d9{
          display: flex;
      }
  @media (max-width: 900px){
      #shopify-section-{{ section.id }}{
          --padding-vertical-d9: {{ section.settings.padding-top-m }}rem;
          --padding-horizontal-d9: {{ section.settings.padding-left-m }}rem;
          --heading-font-d9: {{ section.settings.heading-size-m }}px;
          --text-font-d9: {{ section.settings.text-size-m }}px;
          --btn-font-d9: {{ section.settings.btn-size-m }}px;
          --heading-weight-d9: {% if section.settings.heading-weight-m == 'bold' %}bold{% else %}light{% endif %};
          --btn-weight-d9:  {% if section.settings.btn-weight-m == 'bold' %}bold{% else %}light{% endif %};
  }
  #shopify-section-{{ section.id }} .pi-37-d9-arrows {
      display: {% if section.settings.arrows-m %} flex{% else %}none {% endif %};
      }
      #shopify-section-{{ section.id }}  .pi-37-d9-indicators {
      display:{% if section.settings.dots-m %}flex{% else %}none{% endif %};
      }
      #shopify-section-{{ section.id }}  .pi-37-d9-ingredients p,#shopify-section-{{ section.id }}  .pi-37-d9-ingredients h2{
          text-align: center;
      }
      #shopify-section-{{ section.id }}  .btn-div-d9{
          margin: 0;
      }
      #shopify-section-{{ section.id }}  .pi-37-d9-pills{
          justify-content: center;
      }
      #shopify-section-{{ section.id }}  .hide-on-d-d9{
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      }
      #shopify-section-{{ section.id }}  .hide-on-m-d9{
          display: none;
      }
      #shopify-section-{{ section.id }}  .pi-37-d9-container{
          flex-direction: column;
          gap: 10px;
      }
      #shopify-section-{{ section.id }}  .pi-37-d9-ingredients,  #shopify-section-{{ section.id }}  .pi-37-slider-container{
          width: 100%;
      }
      #shopify-section-{{ section.id }}  .text-div-d9 p{
          margin-top: 30px;
          text-align: center;
      }
  }
</style>
<div class="pi-37-d9-main">
  <div class="pi-37-d9-inner">
    <div class="pi-37-d9-container">
      <div class="pi-37-d9-ingredients">
        <h2>{{ section.settings.heading }}</h2>
        <div class="text-div-d9 hide-on-m-d9">
          {{ section.settings.subtext }}
        </div>
        <div class="pi-37-d9-pills">
          {% for block in section.blocks %}
            {% if block.settings.ingredient_name != blank %}
              <button class="pi-37-d9-pill">{{ block.settings.ingredient_name }}</button>
            {% endif %}
          {% endfor %}
        </div>
        <div class="btn-div-d9 hide-on-m-d9">
          <a
            class="pi-37-d9-main-btn {% if section.settings.btn-1-style == 'btn-2' %} style-btn-2-d9 {% else %}style-btn-1-d9 {% endif %}"
            href="{{ section.settings.btn-link-1 }}"
          >
            {{- section.settings['btn-text-1'] -}}
          </a>
          <a
            class="pi-37-d9-main-btn {% if section.settings.btn-2-style == 'btn-2' %} style-btn-2-d9 {% else %}style-btn-1-d9 {% endif %}"
            href="{{ section.settings.btn-link-2 }}"
          >
            {{- section.settings['btn-text-2'] -}}
          </a>
        </div>
      </div>
      <div class="pi-37-slider-container">
        <div class="pi-37-d9-slider">
          <div class="pi-37-d9-slides">
            {% for block in section.blocks %}
              <div class="pi-37-d9-slide">
                <h2>{{ block.settings.ingredient_name }}</h2>
                {{ block.settings['content-text'] }}
                <div class="img-content-pi-37-d9">
                  <img src="{{ block.settings.content-img |  image_url: width: 1440 }}" alt="">
                </div>
              </div>
            {% endfor %}
          </div>

          <div class="pi-37-d9-indicators">
            {% for block in section.blocks %}
              <span class="pi-37-d9-dot"></span>
            {% endfor %}
          </div>

          <div class="pi-37-d9-arrows">
            <button class="pi-37-d9-arrow pi-37-d9-arrow-left">
              <svg
                version="1.1"
                xmlns="http://www.w3.org/2000/svg"
                xmlns:xlink="http://www.w3.org/1999/xlink"
                x="0px"
