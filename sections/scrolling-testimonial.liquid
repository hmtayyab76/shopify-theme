<style>
   #shopify-section-{{ section.id }}  *{
        padding: 0;
        margin: 0;
        box-sizing: border-box;
    }
    #shopify-section-{{ section.id }}  .main-card-38-d9{
        max-width: 500px;
        width: min-content;
        border: 1px solid {{ section.settings.border-color }};
        border-radius: 10px;
        box-shadow: {{ section.settings.shade-color }} 0px 2px 5px;
        background: {{ section.settings.card-color }};
    }
    #shopify-section-{{ section.id }} .row-card-38-d9{
        display: flex;
        gap: 20px;
        padding: 20px;
    }
    #shopify-section-{{ section.id }} .verification-d9{
        display: flex;
        align-items: center;
        gap: 5px;
    }
    #shopify-section-{{ section.id }} .detail-col-38-d9{
        min-width: 320px;
        display: flex;
        flex-direction: column;
        gap: 6px;
    }
    #shopify-section-{{ section.id }} .img-col-38-d9 {
        overflow: hidden;
        border-radius: 10px;
        min-width: 70px;
        max-width: 70px;
    }
    #shopify-section-{{ section.id }} .img-col-38-d9 img{
        width: 100%;
        height: 100%;
        object-fit: cover;
    }
    #shopify-section-{{ section.id }} .name-38-d9 h4{
        font-size: {{ section.settings.name-size }}px;
        font-weight: 700;
        color: {{ section.settings.color-1 }};
    }
    #shopify-section-{{ section.id }} .detail-col-38-d9 p{
        font-size: {{ section.settings.text-size }}px;
        font-weight: 400;
        line-height: 15.6px;
        color: {{ section.settings.color-2 }};
    }
    #shopify-section-{{ section.id }} .verification-d9 svg{
        width: 16px;
        height: 16px;
    }
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top_mobile }}px;
      padding-bottom: {{ section.settings.padding_bottom_mobile }}px;
      padding-left: {{ section.settings.padding_left_mobile }}px;
      padding-right: {{ section.settings.padding_right_mobile }}px;
      border-radius: 0px;
      background: {{ section.settings.background }}; 
      max-width: fit-content;
      margin: auto;
      margin: 10px 0;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
      padding-left: {{ section.settings.padding_left }}px;
      padding-right: {{ section.settings.padding_right }}px;
    }
    #shopify-section-{{ section.id }}{
    padding: 0px 0px;
  }
  }
  #shopify-section-{{ section.id }} .custom-slider-container-wrapper{
      max-width: fit-content;
      overflow: hidden;
      padding-left: {{ section.settings.padding_left }}px;
      padding-right:{{ section.settings.padding_right }}px;
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom:{{ section.settings.padding_bottom }}px;
      background: transparent !important;
      

    }
    #shopify-section-{{ section.id }} .announcement-text{
      color: white;
    }
    #shopify-section-{{ section.id }} .custom-slider-wrapper{
      display: flex;
      justify-content: flex-start;
      overflow: hidden;
      min-width: max-content;
      margin: auto;
      background: transparent;
    }
    #shopify-section-{{ section.id }} .custom-slider-wrapper-inner{
      display: flex;
      min-width: fit-content;
      width: fit-content;
      animation: {% if section.settings.direction  == 'rtol' %}logoslide-l {% else %}logoslide-r {% endif %} {{ section.settings.smooth_speed }}s infinite cubic-bezier(0, 0, 0, 0);
      gap: {{ section.settings.logos_gap }}px;
      padding-right: {{ section.settings.logos_gap }}px;
    }

#shopify-section-{{ section.id }} .custom-slider-wrapper:hover .custom-slider-wrapper-inner {
  animation-play-state: paused;
}  



#shopify-section-{{ section.id }} .logo-container:hover {
      background: #d8e9de;
}

  
  
    #shopify-section-{{ section.id }} .announcement-link{
      display: flex;
      gap: 10px;
      min-width: fit-content;
      align-items: center;
      justify-content: flex-start;
      padding-right: 100px;
    }

    #shopify-section-{{ section.id }} .announcement-icon{
      padding-left: 100px;
      height: 16px;
    }

    #shopify-section-{{ section.id }} .slide-logo{
      height: auto;
      max-height: 100px;
   /*   min-height: 32px; */
      max-width: 140px;
      object-fit: contain;
    }
    @media (width > 768px) {
      #shopify-section-{{ section.id }} .slide-logo{
        max-height: 120px;
        max-width: 200px;
        max-width: 414px;
        height: 39px;
        width: auto;
      }
    }


  
    #shopify-section-{{ section.id }} .logo-container{
      padding-right: {{ section.settings.logos_gap }}px;
  /*    border:  2px solid red; */
      padding-left: {{ section.settings.logos_gap }}px;
      height: 100%;
      display: flex;
      align-items: center;
      background-color: transparent;
      margin-right:0px;
    }

      #shopify-section-{{ section.id }} .logo-container:hover {
      background-color: transparent;
    }
    
    @keyframes logoslide-l {
      
      0% {
        transform: translateX(0%);
      }
      100% {
        transform: translateX(-100%);
      }
    }
    @keyframes logoslide-r{

      0% {
        transform: translateX(-100%);
      }
      100% {
        transform: translateX(0%);
      }
    }



    #shopify-section-{{ section.id }} .logo-scroll__heading {
      color: {{ section.settings.head-color }};
      font-size: {{ section.settings.head-size }}px !important;
      font-style:;
      font-weight:  {% if section.settings.head-weight == 'normal' %}normal {% elsif section.settings.head-weight == 'light' %} light {% else %}bold{% endif %};
      line-height: normal;
      text-align: center;
      padding-bottom: 30px;
  }

  @media (width > 768px) {
    #shopify-section-{{ section.id }} .logo-scroll__heading {
      font-size: 32px;
    }
  }

  .announcement-bar__logo-scroll {
    padding-bottom: 0;
       {% if section.settings.box-shadow %}
      box-shadow: 4px 4px 20px rgba(191, 191, 191, 0.5);
      {% endif %}
  }
  @media only screen and (max-width: 768px) {
    #shopify-section-{{ section.id }} .logo-container{
      padding-right: {{ section.settings.logos_gap_mobile }}px;
      {% comment %} height: 16px; {% endcomment %}
    }

     
  }

@media screen and (min-width:2500px) {
      #shopify-section-{{ section.id }} .custom-slider-wrapper-inner{
      display: flex;
      min-width: fit-content;
      width: fit-content;
      animation: logoslide 25s infinite cubic-bezier(0, 0, 0, 0);
    }
}
  
@media screen and (min-width:1800px) and (max-width:2499px) {
      #shopify-section-{{ section.id }} .custom-slider-wrapper-inner{
      display: flex;
      min-width: fit-content;
      width: fit-content;
      animation: logoslide 31s infinite cubic-bezier(0, 0, 0, 0);
    }
}
  
@media screen and (max-width:480px) {
  #shopify-section-{{ section.id }} .logo-scroll__heading {
    font-size: {{ section.settings.head-size-m }}px;
  }
  #shopify-section-{{section.id}} {
    /* display:none; */
  }
  #shopify-section-{{ section.id }} .logo-container{
      padding: 0;
    }
  #shopify-section-{{ section.id }} .custom-slider-wrapper-inner{
  }
  #announcement-smooth-{{ section.id }}{
    padding-right: 20px;
  }
}
   #shopify-section-{{ section.id }} a[href]:not([href=""]) {
  cursor: pointer;
}
   #shopify-section-{{ section.id }} a:hover{
    cursor: default;
  }
  
</style>
