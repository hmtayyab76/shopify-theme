{% schema %}
{
    "name": "Scrolling Testimonial",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading"
        },
      {
        "type": "range",
        "id": "head-size",
        "label": "Heading Font size",
        "min": 10,
        "max": 40,
        "step": 1,
        "default": 22,
        "unit": "px"
      },
      {
        "type": "range",
        "id": "head-size-m",
        "label": "Heading Font size for mobile",
        "min": 10,
        "max": 40,
        "step": 1,
        "default": 22,
        "unit": "px"
      },
      {
        "type": "color",
        "id": "head-color",
        "label": "Heading Color",
        "default": "#222"
      },
      {
        "type": "select",
        "id": "head-weight",
        "label": "Font Weight",
        "default": "bold",
        "options": [
          {
            "label": "Light",
            "value": "light"
          },
          {
            "label": "Normal",
            "value": "normal"
          },
          {
            "label": "Bold",
            "value": "bold"
          }
        ]
      },
     

        {
            "type": "image_picker",
            "id": "veri-img",
            "label": "Verified Tick Image"
        },
        {
            "type": "text",
            "id": "veri-text",
            "label": "Verified Text",
            "default": "Verified Buyer"
        },


      {
        "type": "select",
        "id": "direction",
        "label": "Direction of sliding",
        "default": "rtol",
        "options": [
           {
            "label": "Right to Left",
            "value": "rtol"
          },
          {
            "label": "Left to Right",
            "value": "ltor"
          }
        ]
      },
      {
        "type": "checkbox",
        "id": "box-shadow",
        "label": "Enable Box Shadow",
        "default":true
      },
        {
        "type":"number",
        "id":"smooth_speed",
        "label":"Smooth slider speed",
        "default":10
      },
      {
        "type": "header",
        "content": "Desktop configuration"
      },
      {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 2,
      "unit": "px",
      "label": "Top Padding",
      "default": 50
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 2,
      "unit": "px",
      "label": "Bottom Padding",
      "default": 50
    },
    {
      "type": "range",
      "id": "padding_left",
      "min": 0,
      "max": 100,
      "step": 2,
      "unit": "px",
      "label": "Left",
      "default": 50
    },
    {
      "type": "range",
      "id": "padding_right",
      "min": 0,
      "max": 100,
      "step": 2,
      "unit": "px",
      "label": "Right",
      "default": 50
    },
    {
      "type": "range",
      "id": "logos_gap",
      "min": 0,
      "max": 150,
      "step": 5,
      "unit": "px",
      "label": "Logos gap",
      "default": 40
    },
    {
        "type": "range",
        "id": "name-size",
        "label": "Buyer Name Font size",
        "min": 10,
        "max": 40,
        "step": 1,
        "default": 14,
        "unit": "px"
      },
      {
        "type": "range",
        "id": "text-size",
        "label": "text Font size",
        "min": 10,
        "max": 40,
        "step": 1,
        "default": 12,
        "unit": "px"
      },
    {
      "type": "header",
      "content": "Color configuration"
    },
    
    {
        "type": "color_background",
        "id": "background",
        "label": "Background",
        "default": "linear-gradient(#ffffff, #ffffff)"
      },
      {
        "type": "color",
        "id": "card-color",
        "label": "Testimonial Card Color",
        "default": "#fff"
      },
      {
        "type": "color",
        "id": "color-1",
        "label": "Name Color",
        "default": "#BABABA"
      },
      {
        "type": "color",
        "id": "color-2",
        "label": "Review Color",
        "default": "#B1B1B1"
      },
      {
        "type": "color",
        "id": "border-color",
        "label": "Border Color",
        "default": "#EBEBEB"
      },
      {
        "type": "color_background",
        "id": "shade-color",
        "label": "Shadow Color",
        "default": "rgba(115,114,114,0.15)"
      }
    ],
    "blocks": [
        {
