    {%- if section.blocks != blank -%}
      <div class="c-testimonials__rows">
        {% comment %}theme-check-disable{% endcomment %}
        {% assign i = 0 %}
        {% comment %}theme-check-enable{% endcomment %}
        {%- for block in section.blocks -%}
          {% if i <= 2 %}
            {% assign i = i | plus: 1 %}
          {% else %}
            {% assign i = 1 %}
          {% endif %}
          <div
            class="c-testimonial c-testimonial--{{ i }}"
            {% if block.settings.color != blank %}
              style="--testimonial-color:{{ block.settings.color }}"
            {% endif %}
          >
            <div class="c-testimonial__image">
              {%- if block.settings.image_1 != blank -%}
                <img
                  srcset="
                    {%- if block.settings.image_1.width >= 165 -%}{{ block.settings.image_1 | image_url: width: 330 }} 165w,{%- endif -%}
                    {%- if block.settings.image_1.width >= 360 -%}{{ block.settings.image_1 | image_url: width: 720 }} 360w,{%- endif -%}
                    {%- if block.settings.image_1.width >= 533 -%}{{ block.settings.image_1 | image_url: width: 1128 }} 533w,{%- endif -%}
                    {%- if block.settings.image_1.width >= 720 -%}{{ block.settings.image_1 | image_url: width: 1128 }} 720w,{%- endif -%}
                    {%- if block.settings.image_1.width >= 940 -%}{{ block.settings.image_1 | image_url: width: 1128 }} 940w,{%- endif -%}
                    {%- if block.settings.image_1.width >= 1066 -%}{{ block.settings.image_1 | image_url: width: 1128 }} 1066w,{%- endif -%}
                    {{ block.settings.image_1 | image_url }} {{ block.settings.image_1.width }}w
                  "
                  src="{{ block.settings.image_1 | image_url: width: 800 }}"
                  sizes="(min-width: 500px) {{ 1600 | minus: 500 | divided_by: 3 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)"
                  alt="{{ block.settings.image_1.alt | escape }}"
                  class="c-testimonial__img c-testimonial__img--1"
                  loading="lazy"
                  width="{{ block.settings.image_1.width }}"
                  height="{{ block.settings.image_1.height }}"
                >
              {%- endif -%}
              {%- if block.settings.image_2 != blank -%}
                <img
                  srcset="
                    {%- if block.settings.image_2.width >= 165 -%}{{ block.settings.image_2 | image_url: width: 330 }} 165w,{%- endif -%}
                    {%- if block.settings.image_2.width >= 360 -%}{{ block.settings.image_2 | image_url: width: 720 }} 360w,{%- endif -%}
                    {%- if block.settings.image_2.width >= 533 -%}{{ block.settings.image_2 | image_url: width: 1128 }} 533w,{%- endif -%}
                    {%- if block.settings.image_2.width >= 720 -%}{{ block.settings.image_2 | image_url: width: 1128 }} 720w,{%- endif -%}
                    {%- if block.settings.image_2.width >= 940 -%}{{ block.settings.image_2 | image_url: width: 1128 }} 940w,{%- endif -%}
                    {%- if block.settings.image_2.width >= 1066 -%}{{ block.settings.image_2 | image_url: width: 1128 }} 1066w,{%- endif -%}
                    {{ block.settings.image_2 | image_url }} {{ block.settings.image_2.width }}w
                  "
                  src="{{ block.settings.image_2 | image_url: width: 800 }}"
                  sizes="(min-width: 500px) {{ 1600 | minus: 500 | divided_by: 3 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)"
                  alt="{{ block.settings.image_2.alt | escape }}"
                  class="c-testimonial__img c-testimonial__img--2"
                  loading="lazy"
                  width="{{ block.settings.image_2.width }}"
                  height="{{ block.settings.image_2.height }}"
                >
              {%- endif -%}
              {%- if block.settings.image_3 != blank -%}
                <img
                  srcset="
                    {%- if block.settings.image_3.width >= 165 -%}{{ block.settings.image_3 | image_url: width: 330 }} 165w,{%- endif -%}
                    {%- if block.settings.image_3.width >= 360 -%}{{ block.settings.image_3 | image_url: width: 720 }} 360w,{%- endif -%}
                    {%- if block.settings.image_3.width >= 533 -%}{{ block.settings.image_3 | image_url: width: 1128 }} 533w,{%- endif -%}
                    {%- if block.settings.image_3.width >= 720 -%}{{ block.settings.image_3 | image_url: width: 1128 }} 720w,{%- endif -%}
                    {%- if block.settings.image_3.width >= 940 -%}{{ block.settings.image_3 | image_url: width: 1128 }} 940w,{%- endif -%}
                    {%- if block.settings.image_3.width >= 1066 -%}{{ block.settings.image_3 | image_url: width: 1128 }} 1066w,{%- endif -%}
                    {{ block.settings.image_3 | image_url }} {{ block.settings.image_3.width }}w
                  "
                  src="{{ block.settings.image_3 | image_url: width: 800 }}"
                  sizes="(min-width: 500px) {{ 1600 | minus: 500 | divided_by: 3 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)"
                  alt="{{ block.settings.image_3.alt | escape }}"
                  class="c-testimonial__img c-testimonial__img--3"
                  loading="lazy"
                  width="{{ block.settings.image_3.width }}"
                  height="{{ block.settings.image_3.height }}"
                >
              {%- endif -%}
            </div>
            <div class="c-testimonial__content">
              {%- if block.settings.title != blank -%}
                <h3 class="c-testimonial__title">{{ block.settings.title | escape }}</h3>
              {%- endif -%}
              {%- if block.settings.content != blank -%}
                <div class="c-testimonial__text">{{ block.settings.content }}</div>
              {%- endif -%}
              {%- if block.settings.footer != blank -%}
                <div class="c-testimonial__footer">{{ block.settings.footer | escape }}</div>
              {%- endif -%}
              <div class="c-testimonial__rating">
                {% for star in (1..5) %}
                  {%- render 'icon-heart', state: 'active' -%}
                {% endfor %}
              </div>
            </div>
          </div>
        {%- endfor -%}
      </div>
    {%- endif -%}
  </div>
</div>

{% schema %}
{
  "name": "Testimonials",
  "tag": "section",
  "class": "c-testimonials",
  "blocks": [
    {
      "type": "testimonial",
      "name": "Testimonial",
      "settings": [
        {
          "type": "image_picker",
          "id": "image_1",
          "label": "Image 1"
        },
        {
          "type": "image_picker",
          "id": "image_2",
          "label": "Image 2"
        },
        {
          "type": "image_picker",
          "id": "image_3",
          "label": "Image Background"
        },
        {
          "type": "text",
          "id": "title",
          "label": "Title",
          "default": "Daily dose of natural energy for your family’s immunity & vitality."
        },
        {
          "type": "richtext",
          "id": "content",
          "label": "Content",
          "default": "<p>“Excellent choice to stay healthy. We as a family noticed a huge difference, as the immunity has increased and the number of colds and other diseases that usually occur in the winter have been reduced. I recommend everyone use it”. </p>"
        },
        {
          "type": "text",
          "id": "footer",
          "label": "Footer",
          "default": "VERIFIED BUYER, UMF™ 10+"
        },
        {
          "type": "color",
          "id": "color",
          "label": "Color",
          "default": "#E87722"
        }
      ]
    }
  ],
  "settings": [
    {
      "type": "text",
      "id": "title_text",
      "label": "Section Title",
      "default": "Live life in full colors"
    },
    {
      "type": "image_picker",
      "id": "title_image",
      "label": "Graphic title"
    }
  ],
  "presets": [
    {
      "name": "Testimonials"
    }
  ]
}
{% endschema %}
